{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/api/manager/cutoff",
      "destination": "/api/manager/team"
    },
    {
      "source": "/api/debug/team",
      "destination": "/api/kpi/user/debug-team"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
